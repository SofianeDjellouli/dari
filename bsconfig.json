{
  "bs-dependencies": ["@rescript/react", "bs-webapi", "@ryyppy/rescript-promise", "rescript-next"],
  "gentypeconfig": {
    "debug": {
      "all": false,
      "basic": false
    },
    "generatedFileExtension": ".gen.tsx",
    "language": "typescript",
    "shims": {}
  },
  "js-post-build": {
    "cmd": "prettier -w"
  },
  "name": "dari",
  "package-specs": [
    {
      "in-source": true,
      "module": "es6"
    }
  ],
  "ppx-flags": [],
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "app",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js"
}
