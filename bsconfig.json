{
  "bs-dependencies": [
    "@rescript/react",
    "bs-webapi",
    "@ryyppy/rescript-promise",
    "rescript-next",
    "rescript-recoil"
  ],
  "bs-dev-dependencies": ["rescript-test"],
  "gentypeconfig": {
    "debug": {
      "all": false,
      "basic": false
    },
    "generatedFileExtension": ".gen.tsx",
    "language": "typescript",
    "shims": {}
  },
  "js-post-build": {
    "cmd": "prettier -w"
  },
  "name": "dari",
  "package-specs": [
    {
      "in-source": true,
      "module": "commonjs"
    }
  ],
  "ppx-flags": [],
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "app",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "suffix": ".bs.js"
}
