{
  "bs-dependencies": [
    "@rescript/react",
    "bs-webapi",
    "rescript-material-ui",
    "@ryyppy/rescript-promise",
    "@rescriptbr/reform",
    "reschema"
  ],
  "gentypeconfig": {
    "debug": {
      "all": false,
      "basic": false
    },
    "generatedFileExtension": ".gen.tsx",
    "language": "typescript",
    "shims": {}
  },
  "name": "dari",
  "package-specs": [
    {
      "in-source": true,
      "module": "es6"
    }
  ],
  "ppx-flags": ["lenses-ppx/ppx"],
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "app",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js"
}
