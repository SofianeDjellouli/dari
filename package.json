{
  "dependencies": {
    "@ionic/react": "5.9.1",
    "@prisma/client": "3.5.0",
    "@sendgrid/mail": "7.6.0",
    "blitz": "0.43.0",
    "final-form": "4.20.4",
    "react": "18.0.0-alpha-5ca4b0433-20211020",
    "react-dom": "18.0.0-alpha-5ca4b0433-20211020",
    "react-final-form": "6.5.7",
    "recoil": "0.5.2",
    "rescript-recoil": "3.0.0",
    "sass": "1.44.0",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@rescript/react": "^0.10.3",
    "@ryyppy/rescript-promise": "2.1.0",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.37",
    "bs-platform": "^9.0.2",
    "bs-webapi": "^0.19.1",
    "concurrently": "^6.4.0",
    "eslint": "7.32.0",
    "gentype": "^4.3.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.5.0",
    "prettier-plugin-prisma": "3.5.0",
    "pretty-quick": "3.1.2",
    "preview-email": "3.0.5",
    "prisma": "3.5.0",
    "prisma-dbml-generator": "0.8.2",
    "rescript": "^9.1.4",
    "rescript-next": "10.0.0-alpha.1",
    "rescript-test": "3.2.0",
    "typescript": "4.5.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "name": "dari",
  "prettier": {
    "printWidth": 100,
    "semi": false
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "private": true,
  "scripts": {
    "blitz": "docker compose up -d && blitz dev -p 4000",
    "build": "blitz build --debug",
    "dev": "concurrently \"yarn rescript\" \"yarn blitz\" ",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prebuild": "rescript build -with-deps",
    "predev": "rm -f .bsb.lock && rescript clean && docker compose up -d",
    "prepare": "husky install",
    "preprod": "yarn build",
    "pretest": "rescript build",
    "prod": "blitz s -5000",
    "rescript": "rescript build -w",
    "rescript:clean": "rescript clean",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "test": "retest 'tests/*.bs.js' 'tests/**/*.bs.js'"
  },
  "version": "1.0.0"
}
