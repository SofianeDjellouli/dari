    "final-form": "4.20.4",
    "re-formality": "4.0.0-beta.16",
    "react": "18.0.0-alpha-5ca4b0433-20211020",
    "react-dom": "18.0.0-alpha-5ca4b0433-20211020",
    "react-final-form": "6.5.7",
    "reschema": "2.0.3",
    "rescript-material-ui": "^2.1.2",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@rescript/react": "^0.10.3",
    "@ryyppy/rescript-promise": "2.1.0",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.37",
    "bs-platform": "^9.0.2",
    "bs-webapi": "^0.19.1",
    "concurrently": "^6.4.0",
    "eslint": "7.32.0",
    "gentype": "^4.3.0",
    "husky": "7.0.4",
    "lenses-ppx": "6.1.10",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.5.0",
    "prettier-plugin-prisma": "3.5.0",
    "pretty-quick": "3.1.2",
    "preview-email": "3.0.5",
    "prisma": "3.5.0",
    "prisma-dbml-generator": "0.8.2",
    "rescript": "^9.1.4",
    "typescript": "~4.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "name": "dari",
  "prettier": {
    "printWidth": 100,
    "semi": false
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "private": true,
  "scripts": {
    "blitz": "docker compose up -d && blitz dev -p 4000",
    "build": "blitz build",
    "dev": "concurrently \"yarn blitz\" \"yarn rescript\"",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prebuild": "rescript build -with-deps",
    "predev": "rm -f .bsb.lock && docker compose up -d",
    "prepare": "husky install",
    "preprod": "yarn build",
    "prod": "blitz s -5000",
    "rescript": "rm -f .bsb.lock && rescript build -with-deps -w",
    "rescript:clean": "rescript clean",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "version": "1.0.0"
}
